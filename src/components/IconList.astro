---
let iconList = [
    "cloudflare",
    "css3",
    "debian",
    "django",
    "docker",
    "github",
    "go",
    "html5",
    "htmx",
    "javascript",
    "linux",
    "nginx",
    "postgresql",
    "python",
    "react",
    "tailwindcss",
    "typescript",
    "vitejs",
];
iconList.sort((a, b) => a.localeCompare(b));
---

<div
    class="grid grid-cols-6 md:grid-cols-9 xl:grid-cols-18 gap-4 text-2xl text-purple-600 dark:text-purple-400"
>
    {iconList.map((icon) => <i class={`devicon-${icon}-plain`} />)}
</div>
